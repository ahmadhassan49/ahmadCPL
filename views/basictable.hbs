    <script>
        var stroke_active = document.getElementById('stroke-active');
        var stroke_bg = document.getElementById('stroke-bg');
        var output = document.getElementById('output')
        var $output = $("#output"),
            $pie = $(".chart svg #stroke-active");

        update();

        function update() {
            var sliderValue = parseFloat($output.text());
            console.log(sliderValue);
            if (sliderValue < 30) {
                stroke_active.style.stroke = '#bb2d3b9e';
                stroke_bg.style.stroke = '#e71b2e75';
                output.style.fill = '#bb2d3b9e';
            } else if (sliderValue < 70) {
                stroke_active.style.stroke = '#1be7e775';
                stroke_bg.style.stroke = '#1be7e74f';
                output.style.fill = '#1be7e775';
            }
            $output.html(sliderValue + "%");
            // $pie.css({ "stroke-dasharray": 252.2 - sliderValue * 2.522 + " 252.2" });
            $pie.css({ "stroke-dasharray": sliderValue * 2.522 + " 252.2" });
        }
    </script>
    <script>
